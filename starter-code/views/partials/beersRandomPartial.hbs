<div class="random-beer">
  <img src={{this.image_url}} alt="Card image cap">
  <div class="random-text">
    <h5><b>{{this.name}}</b></h5><br>
    <p class="gray">{{this.tagline}}</p><br>
    <p>{{this.description}}</p><br>
    <p><b>Food Pairing</b></p>

    {{#each this.food_pairing}}
    <li>{{this}}</li>
    {{/each}}
    <br>
    <p><b>Brewers Tips: </b>{{this.brewers_tips}}</p><br>

  </div>
</div>